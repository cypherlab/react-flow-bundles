{
  "name": "@cypherlab/react-flow-bundles",
  "version": "0.0.2",
  "description": "Bundles kit for @cypherlab/react-flow",
  "author": "",
  "license": "ISC",
  "repository": "https://github.com/cypherlab/react-flow-bundles",
  "source": "src/index.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "scripts": {
    "watch": "npx rollup -c -w",
    "dev": "NODE_ENV=dev npm run watch",
    "build": "npx rollup -c",
    "prepare": "npm run build",
    "release": "np"
  },
  "files": [
    "dist",
    "src"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "import-directory"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@cypherlab/react-flow": "^0.0.9",
    "babel-plugin-import-directory": "^1.1.0",
    "live-server": "^1.2.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "dependencies": {}
}
